{
  "name": "asp-web-lib-demo",
  "version": "1.0.18",
  "private": false,
  "description": "AspWebLib, Vue 2.0 的桌面端组件库",
  "main": "lib/asp-web-lib.common.js",
  "keyword": "aspire shenzhen front team",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile src/components/theme-chalk/gulpfile.js && cp-cli src/components/theme-chalk/lib lib/theme-chalk",
    "build:utils": "cross-env BABEL_ENV=utils babel src/utils --out-dir lib",
    "lint": "vue-cli-service lint",
    "chore": "webpack --config webpack.common.js",
    "conf": "webpack --config webpack.conf.js",
    "common": "webpack --config build/webpack.common.js",
    "component": "webpack --config build/webpack.component.js --mode production",
    "dist": "npm run common && npm run component && npm run build:utils && npm run build:theme"
  },
  "dependencies": {
    "asp-web-lib-demo": "^1.0.18",
    "aspire-ui111": "^1.0.3",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "compression-webpack-plugin": "^3.1.0",
    "cp-cli": "^1.0.2",
    "cross-env": "^3.1.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.14.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
